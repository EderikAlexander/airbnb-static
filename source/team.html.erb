
<div class="team">
  <ul class="text-center list-inline">
    <h1>Our team</h1>
    <% %w(seb romain anne).each do |name| %>
    <li>
      <h3> <%= name.capitalize %> </h3>
      <%= image_tag "#{name}.jpg", class: "img-circle", width: "100" %>
    </li>
    <% end %>
  </ul>

  <div class="mail-to text-center">
    <%= mail_to "contact@airbnb.com", " contact@airbnb.com",  class:"fa fa-envelope", aria_hidden: "true", font_size: "100"%>
  </div>
</div>
